{
  "manifest_version": 2,

  "name": "My First Image Search Parser",
  "description": "Scrapes Wikimedia Commons for picture",
  "version": "0.1",

  "icons": { "16": "daisy_16.png",
           "48": "daisy_48.png",
          "128": "daisy_128.png" },  
 
  "content_scripts": [
    {
      "matches": ["https://commons.wikimedia.org/*"],
      "js": ["jquery.min.js", "underscore-min.js", "parser.js"]
    }
  ]
}